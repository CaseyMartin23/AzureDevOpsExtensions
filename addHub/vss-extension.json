{
  "manifestVersion": 1,
  "id": "test-hub",
  "version": "0.1.0",
  "name": "test-hub",
  "description": "A test hub",
  "publisher": "CaseyMartin",
  "categories": ["Azure Boards"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/logo.png"
  },
  "contributions": [
    {
      "id": "CaseyMartin.HelloWorld",
      "type": "ms.vss-web.hub",
      "description": "Adds a 'Hello World' hub to the Work hub group.",
      "targets": ["ms.vss-work-web.work-hub-group"],
      "properties": {
        "name": "test-hub",
        "uri": "hello-world.html"
      }
    }
  ],
  "scopes": ["vso.work"],
  "files": [
    {
      "path": "hello-world.html",
      "addressable": true
    },
    {
      "path": "sdk/scripts",
      "addressable": true
    },
    {
      "path": "images/logo.png",
      "addressable": true
    }
  ]
}
