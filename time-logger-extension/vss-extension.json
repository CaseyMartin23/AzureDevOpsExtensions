{
  "manifestVersion": 1,
  "id": "CaseyMartin",
  "version": "0.1.5",
  "name": "Time Logger",
  "description": "A Time Logger for Azure DevOps",
  "publisher": "CaseyMartin",
  "categories": ["Azure Boards"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/logo.png"
  },
  "contributions": [
    {
      "id": "CaseyMartin.HelloWorld",
      "type": "ms.vss-web.hub",
      "description": "Adds a 'Hello' hub to the Work hub group.",
      "targets": ["ms.vss-work-web.work-hub-group"],
      "properties": {
        "name": "Hello Hub",
        "order": 99,
        "uri": "hello-world.html"
      }
    },
    {
      "id": "CaseyMartin.TimeLogger",
      "type": "ms.vss-work-web.work-item-form-page",
      "description": "Custom work item form page",
      "targets": ["ms.vss-work-web.work-item-form"],
      "properties": {
        "name": "The Real Time Logger",
        "uri": "timeLogger.html"
      }
    }
  ],
  "baseUri": "http://localhost:3001/",
  "scopes": ["vso.work"],
  "files": [
    {
      "path": "hello-world.html",
      "addressable": true
    },
    {
      "path": "timeLogger.html",
      "addressable": true
    },
    {
      "path": "sdk/scripts",
      "addressable": true
    },
    {
      "path": "images/logo.png",
      "addressable": true
    }
  ]
}
